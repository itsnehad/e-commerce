{
    "version": 2,
    "functions": {
      "api/*.js": {
        "runtime": "nodejs18.x"
      }
    },
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "dist/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  